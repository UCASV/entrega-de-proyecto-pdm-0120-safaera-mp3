        <activity android:name=".login.RegisterActivity" />
        <activity android:name=".login.Presentation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>